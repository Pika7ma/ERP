<Window x:Class="ERP.UI.Inventory.TransferStorage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="仓库跳转" Height="300" Width="300">
    <Grid>
        <Label Content="跳转至" HorizontalAlignment="Left" Margin="37,80,0,0" VerticalAlignment="Top"/>

        <Button Content="确定" HorizontalAlignment="Left" Margin="55,160,0,0" VerticalAlignment="Top" Width="57"/>
        <Button Content="取消" HorizontalAlignment="Left" Margin="166,160,0,0" VerticalAlignment="Top" Width="57" RenderTransformOrigin="0.5,0.5">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.888"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Label Content="号仓库" Margin="196,80,51.6,0" VerticalAlignment="Top"/>
        <ComboBox HorizontalAlignment="Left" Margin="146,80,0,0" VerticalAlignment="Top" Width="45" RenderTransformOrigin="0.921,0.865"/>

    </Grid>
</Window>